{
    "name": "ongr/elasticsearch-dsl",
    "description": "Elasticsearch DSL library",
    "type": "library",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "require": {
